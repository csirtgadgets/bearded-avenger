[supervisord]
loglevel = ERROR


[program:cif-router]
command = cif-router
autostart = true
stderr_logfile = {{ shared_path/log }}/cif-router.log

[program:cif-httpd]
command = cif-httpd
autostart = true
stderr_logfile = {{ shared_path/log }}/cif-httpd.log

[program:csirtg-smrt]
command = csirtg-smrt
autostart = true
stderr_logfile = {{ shared_path/log }}/csirtg-smrt.log
